[#Module new: "socket]
[s =! [[socket @ Socket] new: ([[socket @ Socket] @ #AF_INET], [[socket @ Socket] @ #SOCK_STREAM])]]
[s bind: ((0 0 0 0), 0)]
[s connect: ((140 211 15 34), 80)]
[s write: "`GET /index.html HTTP/1.1\r\nHost: www.debian.org\r\n\r\n']
[data =! "`']
[[[[r =! [s read: 65536]] equal: "`'] not] &while:
	[data = ["`' join: (data r)]]
]
