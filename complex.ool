
[#Metaclass new: "Complex parent: #Object instance-variables: "(re im)]

[[Complex class-methods] @ new: =
	  {(cl args) [z =! [[[[#Object class-methods] @ new] cdr] eval: (cl)]]
	             [z @ re = [args car]]
		     [z @ im = [args cdr]]
		     z
	  }
]

[[Complex class-methods] @ new = {(cl) [Complex new: (0, 0)]}]

[z1 =! [Complex new]]

[z1 @ re]
[z1 @ im]

[z2 =! [Complex new: (13, -42)]]

[z2 @ re]
[z2 @ im]
